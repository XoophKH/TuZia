@model List<esame.Models.FroggerViewModel>

<h1>Punteggio</h1>
@if(Model.Count != 0)
 {   
<hr />
        int max = int.MinValue;

        @foreach (var type in Model)
            {
            if (type.Punteggio > max)
                {
                max = type.Punteggio;
                }
            }
            <h3>Record = @max</h3>

    <hr />
<ul>
    
@foreach(var frog in Model)
{
    <li>@frog.Punteggio</li>

}

</ul>
      }
  
      
                    

    <a class="btn btn-primary" href="/Frogger/Inserct" class="Button">Insersci un valore</a>
    

